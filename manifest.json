{
    "manifest_version": 2,
    "name": "Pass Generator",
    "version": "1.0",
    "description": "12 char basic password generator.",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Pass Generator",
      "default_popup": "popup.html"
    },
    "permissions": ["clipboardWrite"]
  }
  